{% extends '::frontend.html.twig' %}

{% block body -%}

    <div class="row">
    	<div class="col-xs-12 col-sm-12 com-md-12">
        	<h2><a href="{{ path('procedencia') }}">
            	Procedencia:
        	</a>
        	<span>
            	Nuevo
        	</span></h2>
    	</div>
	</div>

	<div class="row">
		<div class="col-xs-12 col-sm-12 col-md-12">
        	<div class="portlet light tasks-widget ">
            	<div class="portlet-title">
                	<div class="caption">
                		<i class="icon-share font-blue"></i>
                        	<span class="caption-subject font-green bold uppercase">Nueva Procedencia</span>
                	</div>
            	</div>
            	<div class="portlet-body">
                	<div class="task-content">
                		{{ form_start(form) }}

                			<div class="form-group">
	    						<div class="row">
	    							<div class="col-xs-12 col-sm-2 col-md-2">
	                    				Fecha Llegada:
	                    			</div>
	                    			<div class="col-xs-12 col-sm-3 col-md-3">
	                    				{{ form_widget(form.fechaLlegada) }}
	                    			</div>
	                			</div>
	            			</div>
	            			<div class="form-group">
	    						<div class="row">
	    							<div class="col-xs-12 col-sm-2 col-md-2">
	                    				Numero de Serie Fabricante:
	                    			</div>
	                    			<div class="col-xs-12 col-sm-3 col-md-3">
	                    				{{ form_widget(form.numeroSerieFabricante) }}
	                    			</div>
	                			</div>
	            			</div>
	            			<div class="form-group">
	    						<div class="row">
	    							<div class="col-xs-12 col-sm-2 col-md-2">
	                    				Numero de Serie Warriors:
	                    			</div>
	                    			<div class="col-xs-12 col-sm-3 col-md-3">
	                    				{{ form_widget(form.numeroSerieWarriors) }}
	                    			</div>
	                			</div>
	            			</div>
	            			<div class="form-group">
	    						<div class="row">
	    							<div class="col-xs-12 col-sm-2 col-md-2">
	                    				Fecha Salida:
	                    			</div>
	                    			<div class="col-xs-12 col-sm-3 col-md-3">
	                    				{{ form_widget(form.fechaSalida) }}
	                    			</div>
	                			</div>
	            			</div>
	            			<div class="form-group">
	    						<div class="row">
	    							<div class="col-xs-12 col-sm-2 col-md-2">
	                    				Fabricante:
	                    			</div>
	                    			<div class="col-xs-12 col-sm-3 col-md-3">
	                    				{{ form_widget(form.fabricante,{"attr":{"class":"form-control"}}) }}
	                    			</div>
	                			</div>
	            			</div>
	            			
						{{ form_end(form) }}
            			
            		</div>
		        </div>
		    </div>
		</div>
	</div>

{% endblock %}
