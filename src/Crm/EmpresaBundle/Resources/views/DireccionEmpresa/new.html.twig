{% extends '::frontend.html.twig' %}

{% block body -%}
    <div class="row">
    	<div class="col-xs-12 col-sm-12 col-md-12">
    		<h2><a href="{{ path('empresa') }}">
                Empresa
    		</a>
    		<span>
    			Nuevo
    		</span></h2>
    	</div>
    </div>

    <div class="row">
    	<div class="col-xs-12 col-sm-12 col-md-12">
    		<div class="portlet light tasks-widget">
    			<div class="portlet-title">
    				<div class="caption">
    					<i class="icon-share font-blue"></i>
                        <span class="caption-subject font-green bold uppercase">Nueva Empresa</span>
    				</div>
    			</div>

    			<div class="portlet-body">
    				<div class="task-content">

                        {{ form_start(form) }}
                         <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Razon Socail:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.razonSocial)}}
                                    </div>
                                </div>
                            </div>
                             <div class="form-group">
                               <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        RFC:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.rfc) }}
                                    </div>
                                </div>
                            </div>                      
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Telefono 1:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.telefono1) }}
                                    </div>
                                
                                    <div class="col-xs-12 col-sm-2 col-md-1">
                                        Extencion:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.ext1) }}
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Telefono 2:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.telefono2) }}
                                    </div>
                                
                                    <div class="col-xs-12 col-sm-2 col-md-1">
                                        Extencion:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.ext2) }}
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        E-mail:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.email) }}
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Pagina web:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.paginaWeb) }}
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Estatus:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.empresa.estatus, {"attr":{"class":"form-control"},"requeried":"true"}) }}
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Calle:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.calle)}}
                            		</div>
                            	</div>
                            </div>
                             <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Numero exterior:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.numeroExt)}}
                            		</div>
                            	</div>
                            </div>
                             <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Numero interior:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.numeroInt)}}
                            		</div>
                            	</div>
                            </div>
                             <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Colonia:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.colonia)}}
                            		</div>
                            	</div>
                            </div>
                             <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Codigo postal:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.codigoPostal)}}
                            		</div>
                            	</div>
                            </div>
                            <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Pais:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.pais,{"attr":{"class":"form-control"},"required":"true"}) }}
                            		</div>
                            	</div>
                            </div>
                            <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Estado:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.estado,{"attr":{"class":"form-control"},"required":"true"}) }}
                            		</div>
                            	</div>
                            </div>
                            <div class="form-group">
                            	<div class="row">
                            		<div class="col-xs-12 col-sm-2 col-md-2">
                            			Municipio:
                            		</div>
                            		<div class="col-xs-12 col-sm-3 col-md-3">
                            			{{ form_widget(form.municipio,{"attr":{"class":"form-control"},"required":"true"}) }}
                            		</div>
                            	</div>
                            </div>
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-2 col-md-2">
                                        Localidad:
                                    </div>
                                    <div class="col-xs-12 col-sm-3 col-md-3">
                                        {{ form_widget(form.localidad,{"attr":{"class":"form-control"},"required":"true"}) }}
                                    </div>
                                </div>
                            </div>
                        {{ form_end(form) }}    
                    </div>
                </div>
            </div> 
        </div>  
     </div>  
{% endblock %}
