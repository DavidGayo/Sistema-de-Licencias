{% extends '::frontend.html.twig' %}

{% block body -%}

    <div class="row">
        <div class="col-md-12">
            <div class="portlet light ">
                <div class="portlet-title">
                    <div class="caption font-dark">
                        <i class="icon-settings font-dark"></i>
                            <span class="caption-subject bold uppercase"> Lista Capacidad</span>
                    </div>
                </div>
                <div class="portlet-body">
                    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Capacidad</th>
                                <th>Actions</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for entity in entities %}
                                <tr>
                                    <td>
                                        <a href="{{ path('capacidad_show', { 'id': entity.id }) }}">{{ entity.id }}</a>
                                    </td>
                                    <td>
                                        {{ entity.capacidad }}
                                    </td>
                                    <td>
                
                                        <a href="{{ path('capacidad_show', { 'id': entity.id }) }}" class="btn yellow btn-outline">show</a>
                                        <a href="{{ path('capacidad_edit', { 'id': entity.id }) }}" class="btn blue btn-outline">edit</a> 
                                    </td>
                                </tr>
                            {% endfor %}
                        </tbody>
                    </table>
                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12">
                            <a href="{{ path('capacidad_new') }}" class="btn green btn-outline">
                                Create a new entry
                            </a>
                        </div>
                    </div>
                                        
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block javascript%}
        
{% endblock javascript%}
