{% extends '::frontend.html.twig' %}

{% block body -%}
    <h1>EspecificacionesEquipo list</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Numerointerfaces</th>
                <th>Numerousb</th>
                <th>Expansion</th>
                <th>procesador</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('especificacionesequipo_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.numeroInterfaces }}</td>
                <td>{{ entity.numeroUsb }}</td>
                <td>{{ entity.expansion }}</td>
                <td>{{ entity.procesador }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('especificacionesequipo_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('especificacionesequipo_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('especificacionesequipo_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
