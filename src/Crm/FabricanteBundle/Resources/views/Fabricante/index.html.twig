{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Fabricante list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Razonsocial</th>
                <th>Rfc</th>
                <th>Telefono1</th>
                <th>Ext1</th>
                <th>Telefono2</th>
                <th>Ext2</th>
                <th>Email</th>
                <th>Paginaweb</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('fabricante_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.razonSocial }}</td>
                <td>{{ entity.rfc }}</td>
                <td>{{ entity.telefono1 }}</td>
                <td>{{ entity.ext1 }}</td>
                <td>{{ entity.telefono2 }}</td>
                <td>{{ entity.ext2 }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.paginaWeb }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('fabricante_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('fabricante_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('fabricante_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
